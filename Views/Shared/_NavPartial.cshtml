@{
    Layout = null;
    var controller = ViewBag.User as string;
}



<style>
    .header-nav {
    display: flex;
    gap: 1.5rem;
    }

    .nav-link {
    color: var(--gray-600);
    text-decoration: none;
    transition: var(--transition);
    }

    .nav-link:hover {
    color: var(--primary-color);
    }

    /* ---- Chat Layout Styles ---- */
    .chat-layout {
    height: 100% !important;
    display: flex;
    height: calc(100% - 57px); /* subtract header height */
    overflow: hidden;
    }

    /* Navigation Sidebar */
    .nav-sidebar {
    height: 100%;
    width: 76px;
    flex-shrink: 0;
    background-color: #9370db !important;
    border-right: 1px solid var(--dark-light);
    display: flex;
    flex-direction: column;
    align-items: center;
    padding: 1rem 0;
    color: var(--white);
    }

    .logo-container {
    padding: 0.5rem 0 1.5rem;
    }

    img.sidebar-img {
    height: 45px;
    width: 45px;
    border-radius: 10px;
    }

    .chat-logo-icon {
    width: 32px;
    height: 32px;
    color: var(--white);
    opacity: 0.7;
    transition: var(--transition);
    cursor: pointer;
    }

    .chat-logo-icon:hover {
    opacity: 1;
    }

    .sidebar-nav {
    flex: 1;
    display: flex;
    flex-direction: column;
    justify-content: center; /* Vertically center icons */
    width: 100%; /* Ensure full width */
    }

    .nav-list {
    list-style: none;
    display: flex;
    flex-direction: column;
    align-items: center; /* Horizontally center each icon */
    gap: 1.5rem;
    padding: 0;
    margin: 0;
    }


    .nav-item {
    display: flex;
    flex-direction: column;
    align-items: center;
    position: relative;
    cursor: pointer;
    }

    .nav-icon {
    width: 24px;
    height: 24px;
    color: #ced4da;
    transition: var(--transition);
    }

    .nav-text {
    font-size: 0.75rem;
    margin-top: 0.25rem;
    color: var(--gray-400);
    }

    .nav-item:hover .nav-icon,
    .nav-item:hover .nav-text {
    color: #ffffff;
    }


    .nav-item.active .nav-icon,
    .nav-item.active .nav-text {
    color: white;
    }


    .user-profile {
    margin-top: auto;
    position: relative;
    }

    .badge {
    position: absolute;
    top: -5px;
    right: -5px;
    background: red;
    color: white;
    border-radius: 50%;
    padding: 2px 6px;
    font-size: 0.7em;
    min-width: 18px;
    text-align: center;
    }

    .d-none {
    display: none !important;
    }

    /* Toast styles */
    .toast {
    margin-bottom: 10px;
    max-width: 350px;
    }

    .toast-header.bg-low {
    background-color: #0d6efd !important;
    }

    .toast-header.bg-medium {
    background-color: #ffc107 !important;
    color: #000 !important;
    }

    .toast-header.bg-high {
    background-color: #dc3545 !important;
    }
</style>


