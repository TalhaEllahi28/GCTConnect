 *@
<script src="https://cdnjs.cloudflare.com/ajax/libs/microsoft-signalr/8.0.0/signalr.min.js"></script>

 <script>
    // Initialize with server-side count
    let announcementCount = @ViewBag.UnreadAnnouncements;
    updateAnnouncementBadge();

    // Enhanced connection handling
    let connection = new signalR.HubConnectionBuilder()
        .withUrl("/Services/ChatHub", {
            withCredentials: true,
            skipNegotiation: true,
            transport: signalR.HttpTransportType.WebSockets
        })
        .withAutomaticReconnect()
        .build();

    connection.on("receiveannouncement", (title, content, priority) => {
        console.log("New announcement:", title);

        // Update badge
        announcementCount++;
        updateAnnouncementBadge();

        // Show modal notification
        window.showAnnouncementModal(title, content, priority);
    });

    // Start connection
    connection.start()
        .then(() => console.log("SignalR connected"))
        .catch(err => console.error("Connection failed:", err));

    // Update badge display
    function updateAnnouncementBadge() {
        const badge = document.getElementById("announcement-badge");
        if (announcementCount > 0) {
            badge.innerText = announcementCount;
            badge.classList.remove("d-none");
        } else {
            badge.classList.add("d-none");
        }
    }

    // Fetch and display all announcements
    window.loadAnnouncements = async function() {
        try {
            const container = document.getElementById("announcementsContainer");
            container.innerHTML = '<div class="loading-spinner">              </div>';

            const response = await fetch('/Home/GetUnreadAnnouncements');
            const announcements = await response.json();

            renderAnnouncements(announcements);
        } catch (error) {
            console.error('Error loading announcements:', error);
            container.innerHTML = '<div class="error-message">Error loading announcements</div>';
        }
    };

    // Render announcements in modal
    function renderAnnouncements(announcements) {
        const container = document.getElementById("announcementsContainer");
        container.innerHTML = '';

        if (!announcements || announcements.length === 0) {
            container.innerHTML = '<div class="empty-message">No unread announcements</div>';
            return;
        }

        announcements.forEach(ann => {
            const announcementEl = document.createElement("div");
            announcementEl.className = "announcement-card";
            announcementEl.innerHTML = `
                <div class="announcement-header">
                    <h4>${ann.title}</h4>
                    <span class="priority-badge" data-priority="${ann.priority}">${ann.priority}</span>
                </div>
                <div class="announcement-content">
                    <p>${ann.content}</p>
                    <div class="announcement-meta">
                        <small>${new Date(ann.createdAt).toLocaleString()}</small>
                    </div>
                </div>
            `;
            container.appendChild(announcementEl);
        });
    }

    // Mark announcements as read
    window.markAnnouncementsRead = async function() {
        try {
            await fetch('/Home/MarkAsRead', { method: 'POST' });

            // Reset badge count
            announcementCount = 0;
            updateAnnouncementBadge();

            console.log("Announcements marked as read");
        } catch (err) {
            console.error('Mark as read failed:', err);
        }
    };

    // Handle announcement icon click
    document.querySelector('#announcement-icon').addEventListener('click', function(e) {
        e.preventDefault();
        window.showAllAnnouncementsModal();
        window.markAnnouncementsRead();
    });
</script>


